type Channel {
  id: ID!
  color: String!
  icon: String!
  initials: String!
  isPos: Boolean!
  name: String!
}

type ProfileChannelAssociation {
  id: ID!
  name: String!
}

extend type Order @key(fields: "id")  {
  id: ID! @external
  accountId: Int!
  channelId: Int!
  account: ProfileChannelAssociation
  channel: Channel
}
